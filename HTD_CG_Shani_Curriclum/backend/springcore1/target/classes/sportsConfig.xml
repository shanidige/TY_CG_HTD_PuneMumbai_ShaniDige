<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sports" class="com.capgemini.springcore1.beans.Sports">
		<property name="name" value="Archery" />
		<property name="totalPlayers" value="4" />
		<property name="teamInfo">
			<map>
				<entry key="INDIA" value="Sachin" />
				<entry key="PAKISTAN" value="Shahid" />
				<entry key="Australia" value="Arron" />
			</map>
		</property>
	</bean>

</beans>